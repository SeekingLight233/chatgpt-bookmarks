{
  "name": "chatgpt-bookmarks",
  "displayName": "Chatgpt bookmarks",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "SeekingLight",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "format": "prettier --write ."
  },
  "dependencies": {
    "ahooks": "^3.7.7",
    "plasmo": "0.69.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resso": "^0.14.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "@types/react": "18.0.32",
    "@types/react-dom": "18.0.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "5.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "yarn format"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "tabs"
    ]
  }
}
